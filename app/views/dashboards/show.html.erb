<div class="scroll">
  <div class="top-dasboard">
  <h1 style="color:white; margin-left: 15px;"><strong>Mon dashboard</strong></h1>
  </div>


  <div class="container-follow mt-4">
    <h2 class= "dashboard-title">Calls à venir </h2>
    <div class="card-dashboard d-flex">
      <% unless @accepted_matches.nil? %>
        <% @accepted_matches.each do |match| %>
          <div class= "div-notification d-flex align-items-center" type = "button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
            <img src="https://files.helpdocs.io/zqc32mkmew/logo.png?t=1608128023138"/>
            <div>
              <p class="call">16 avril 2022 à 13h00 </p>
              <p class="call">jusqu'a 14H00</p>
            </div>
          </div>
        <% end %>
      <% end %>
    </div>
    <div class="card-dashboard d-flex">
      <img src="https://www.appvizer.fr/media/application/26182/logo/logo-skello.png" />
      <div class="">
        <p class="call">16 avril 2022 à 13h00 </p>
        <p class="call">jusqu'a 14H00</p>
      </div>
    </div>
  </div>

  <div class="container-follow">
    <h2 class= "dashboard-title">Matchs en attente </h2>
    <div class="card-dashboard-wait d-flex justify-content-around">
      <% unless @pending_matches.nil?  %>
        <% @pending_matches.each do |match| %>
            <div class= "div-notification" type = "button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
              <img src="https://files.helpdocs.io/zqc32mkmew/logo.png?t=1608128023138"/>
            </div>
        <% end %>
      <% end %>

      <% unless @validating_matches.nil? %>
        <% @validating_matches.each do | match | %>
          <div class= "div-notification" type = "button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
            <div class= "button-notification"> 1 </div>
            <img src="https://files.helpdocs.io/zqc32mkmew/logo.png?t=1608128023138"/>
          </div>
        <% end %>
      <% end %>

      <img src="https://touteslesbox.fr/wp-content/uploads/2019/02/hellofresh-logo.jpg"/>
      <img src="data:image/png;base64,"/>
    </div>
  </div>

  <div class="calendar-container mt-4">
   <h2 class= "dashboard-title">Mon agenda </h2>
    <%= month_calendar(events: @matches, attribute: :callbooking) do |day , match| %>
    <%= day %>
    <%= match.first.job_offer.company.name if match.any? %>
    <%= match.first.callbooking.strftime('%H:%M') if match.any? %>
    <% end %>
  </div>
</div>

<!--MODAL-->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog m-0">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Veuillez sélectionner un horaire</h5>
      </div>
      <div class="modal-body">
          <div class="btn-book">
            <%= link_to "14/03/21- 16h/16h30", book_date_path %>
          </div>
          <div class="btn-book">
            <%= link_to "09/03/21- 14h30/15h", book_date_path %>
          </div>
          <div class="btn-book">
            <%= link_to "17/03/21- 10h30/11h", book_date_path %>
          </div>
          <div class="btn-book">
            <%= link_to "10/03/21- 15h30/16h", book_date_path %>
          </div>
           <div class="btn-book btn-delete">
            <%= link_to "Annuler le call", book_date_path %>
          </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Fermer</button>
      </div>
    </div>
  </div>
</div>
