
<div class="container-follow mt-4">
  <h2>Match en attentes</h2>
  <div class="card-dashboard d-flex justify-content-around mt-4">
    <img src="https://www.appvizer.fr/media/application/26182/logo/logo-skello.png" />
    <img src="https://cdn.1min30.com/wp-content/uploads/2018/02/Couleur-logo-Airbnb.jpg" />
    <img src="https://www.vizir.co/wp-content/uploads/2021/07/logo-rond-payfit.png"/>
  </div>
</div>

<div class="container-follow mt-4">
  <h2>En attente de validations questions</h2>
  <div class="card-dashboard d-flex justify-content-around mt-4">
  <% @matches.each do |match| %>
    <% if match.status == true && match.callbooking == nil %>
    <div class= "div-notification">
      <div class= "button-notification"> 1 </div>
      <img src="https://files.helpdocs.io/zqc32mkmew/logo.png?t=1608128023138"/>
    </div>
    <% end %>
  <% end %>
    <img src="https://touteslesbox.fr/wp-content/uploads/2019/02/hellofresh-logo.jpg"/>
    <img src="data:image/png;base64,"/>
  </div>
</div>

<div class="calendar-container mt-4">
  <%= month_calendar(events: @meetings, end_attribute: :end_time) do |date, meetings| %>
    <%= date %>
    <% meetings.each do |meeting| %>
      <div class="meeting">
        <p><%= meeting.name %></p>
        <small><%= meeting.time unless meeting.multi_days? %></small>
      </div>
    <% end %>
  <% end %>
</div>
